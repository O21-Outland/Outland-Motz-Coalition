<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="OutlandSculptureBase">
		<defName>Outland_SculptureOxectGroup</defName>
		<label>grand oxect sculpture</label>
		<description>A huge sculpture sized to dominate a room.</description>
		<castEdgeShadows>true</castEdgeShadows>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>Outland/Motz/Buildings/SculptureGrandOxect</texPath>
			<drawSize>(4,4)</drawSize>
		</graphicData>
		<fillPercent>0.5</fillPercent>
		<rotatable>false</rotatable>
		<size>(2,2)</size>
		<canOverlapZones>false</canOverlapZones>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<Beauty>400</Beauty>
			<Mass>28</Mass>
			<WorkToBuild>15000</WorkToBuild>
			<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">40</StyleDominance>
		</statBases>
		<building>
			<paintable>true</paintable>
		</building>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>400</costStuffCount>
		<designationCategory>Outland_Outland</designationCategory>
		<modExtensions>
			<li Class="TabulaRasa.DefModExt_SubCategoryBuilding">
				<subCategory>Outland_SubCatDecoration</subCategory>
				<showOnlyInCategory>true</showOnlyInCategory>
			</li>
		</modExtensions>
	</ThingDef>

</Defs>